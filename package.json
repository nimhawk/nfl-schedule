{
  "name": "nfl-schedule",
  "version": "0.0.1",
  "description": "Playground App for displaying (Patriots) 2015 NFL schedule/results",
  "main": "server.js",
  "scripts": {
    "prepublish": "npm run build:js && npm run build:css && npm run build:dev",
    "build:js": "babel src -d lib --optional runtime",
    "build:css": "node-sass src/ -o lib/",
    "build:dev": "webpack -d --progress --colors",
    "start": "node devserver",
    "test": "npm run be-test",
    "be-test": "sh -c 'npm run be-test-cover && istanbul check-coverage --statements 60 --functions 60 --branches 60 --lines 60'",
    "be-test-cover": "env NODE_ENV=development istanbul cover _mocha -- -r test/_helpers/boot $(find test src -type f -name '*-test.js')",
    "be-test-watch": "env NODE_ENV=development mocha -G -w -R min -t 300 -r test/_helpers/boot $(find test src -type f -name '*-test.js')"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nimhawk/nfl-schedule"
  },
  "keywords": [
    "nfl",
    "new england",
    "patriots"
  ],
  "author": "nimit.barochia@gmail.com",
  "license": "MIT",
  "dependencies": {
    "async": "~1.4.2",
    "babel": "5.6.14",
    "babel-core": "~5.8.24",
    "babel-runtime": "5.7.0",
    "cheerio": "~1.2.0",
    "common-errors": "^0.4.18",
    "express": "~4.11.2",
    "jquery": "~2.1.4",
    "lodash": "~3.6.0",
    "morgan": "~1.5.1",
    "react": "~0.13.3",
    "request": "~2.61.0",
    "serve-static": "^1.8.1"
  },
  "devDependencies": {
    "babel-loader": "~5.3.2",
    "css-loader": "~0.18.0",
    "extract-text-webpack-plugin": "~0.8.2",
    "istanbul": "~0.3.13",
    "jsdom": "~3.1.2",
    "json-loader": "~0.5.2",
    "mocha": "~2.2.3",
    "node-sass": "3.2.0",
    "proxyquire": "~1.4.0",
    "react-tools": "~0.13.3",
    "require-directory": "~2.1.0",
    "sass-loader": "~2.0.1",
    "should": "~7.0.2",
    "sinon": "~1.14.1",
    "style-loader": "~0.12.3",
    "webpack": "~1.12.1",
    "webpack-dev-server": "~1.10.1"
  }
}
